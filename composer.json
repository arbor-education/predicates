{
    "name": "dutekvejin/predicates",
    "description": "Collection handling in PHP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "dutekvejin",
        "predicates"
    ],
    "authors": [
        {
            "name": "Dusan Vejin",
            "email": "dutekvejin@gmail.com",
            "homepage": "https://github.com/dutekvejin"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "dutekvejin/transformers": "^0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3|^8.0",
        "friendsofphp/php-cs-fixer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Dutek\\Predicate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dutek\\Predicate\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "cs-fix": "php-cs-fixer fix --config=.php-cs-fixer.php",
        "cs-check": "php-cs-fixer fix --config=.php-cs-fixer.php -v --diff --dry-run"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
